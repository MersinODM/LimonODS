{
    "private": true,
    "scripts": {
        "dev-all": "concurrently \"npm --section=app run dev\" \"npm --section=exam run dev\"  --kill-others-on-fail",
        "dev-app": "npm run dev --section=app",
        "dev-exam": "npm run dev --section=exam",
        "watch-app": "npm run watch  --section=app",
        "watch-exam": "npm run watch  --section=exam",
        "prod-app": "npm run prod --section=app",
        "prod-exam": "npm run exam --section=exam",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
